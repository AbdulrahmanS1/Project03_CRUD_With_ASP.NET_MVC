@model ServiceModel



<h1>Create Service page</h1>

<form asp-action="Create" method="post" class="form-horizantal">

    <div class="form-group">
        <label asp-for="Service_Id">Id</label>
        <div>
            <input asp-for="Service_Id" />
        </div>
    </div>

    <div class="form-group">
        <label asp-for="Service_Name">Name</label>
        <div>
            <input asp-for="Service_Name" />
        </div>
    </div>

    <div class="form-group">
        <label asp-for="StaffId">Staff</label>
        <div>
            <input asp-for="StaffId" />
            <span asp-validation-for="StaffId" class="alert-danger"></span>
        </div>
    </div>
    <div class="form-group">
        <input type="submit" class="btn-btn-primary" value="Submit" />
    </div>
</form>